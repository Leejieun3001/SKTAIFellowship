<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mapbox_test">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/flashbase_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/flashbase_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LocationComponentCameraOptionsActivity"></activity>
        <activity android:name=".SymbolSwitchOnZoomActivity" />
        <activity android:name=".InfoWindowSymbolLayerActivity" />
        <activity android:name=".SymbolCollisionDetectionActivity" />
        <activity android:name=".PulsingLayerOpacityColorActivity" />
        <activity android:name=".LanguageSwitchActivity" />
        <activity android:name=".SecondActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>